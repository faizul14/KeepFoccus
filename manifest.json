{
  "manifest_version": 2,
  "name": "Save You'r Brain",
  "version": "1.0",
  "description": "block websites to save brain.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["codebg.js"],
    "persistent": true
  },
  "web_accessible_resources": ["redirect.html", "listblock.json"]
}
